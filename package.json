{
  "name": "shopee",
  "version": "0.0.1",
  "description": "shopee app",
  "scripts": {
    "start": "react-native start",
    "web": "NODE_ENV=development webpack-dev-server",
    "web:dist": "rm -rf dist && NODE_ENV=production webpack",
    "lint": "tslint -p . && tsc --noEmit --skipLibCheck",
    "build:iOS": "react-native bundle --entry-file index.js --bundle-output ./ios/bundle/index.ios.bundle --platform ios --assets-dest ./ios/bundle --dev false",
    "build:android": "react-native bundle --entry-file index.js --bundle-output ./android/app/src/main/assets/index.android.bundle --platform android --assets-dest ./android/app/src/main/res --dev false",
    "build": "npm run build:android && npm run build:iOS",
    "precommit": "npm run lint",
    "test": "jest --forceExit",
    "postinstall": "node script/react-scrollview-fix.js"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.8.1",
    "@react-native-community/masked-view": "^0.1.7",
    "@types/config": "0.0.36",
    "@types/koa": "^2.11.2",
    "@types/koa-router": "^7.4.0",
    "@types/lodash": "^4.14.149",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.5",
    "@types/react-native": "^0.61.0",
    "@types/react-native-i18n": "^2.0.0",
    "@types/react-test-renderer": "^16.9.2",
    "config": "^3.3.0",
    "lodash": "^4.17.15",
    "mobx": "^5.15.4",
    "mobx-react": "^6.1.8",
    "number-precision": "^1.3.2",
    "react": "^16.9.0",
    "react-dom": "^16.13.0",
    "react-native": "^0.61.5",
    "react-native-device-info": "^5.5.3",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-i18n": "^2.0.15",
    "react-native-largelist-v3": "^3.0.15",
    "react-native-picker-select": "^6.6.0",
    "react-native-reanimated": "^1.7.0",
    "react-native-safe-area-context": "^0.7.3",
    "react-native-screens": "^2.3.0",
    "react-native-spring-scrollview": "^2.0.23",
    "react-native-svg": "^12.0.3",
    "react-native-swipeout": "^2.3.6",
    "react-native-typescript-transformer": "^1.2.13",
    "react-native-web": "^0.12.2",
    "react-navigation": "^4.2.2",
    "react-navigation-drawer": "^2.4.7",
    "react-navigation-stack": "^2.2.3",
    "recyclerlistview": "^3.0.0",
    "tslib": "^1.11.1"
