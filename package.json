    "rimraf": "^3.0.2",
    "ts-loader": "^6.2.1",
    "tslint": "^6.0.0",
    "tslint-react": "^4.2.0",
    "typescript": "^3.8.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "web-webpack-plugin": "^4.6.7",
    "webpack": "^4.42.0",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "jest": {
    "preset": "react-native"
  }
}
